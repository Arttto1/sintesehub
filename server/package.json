    "dotenv": "^17.2.0",
    "google-auth-library": "^10.1.0",
    "graphql": "^16.11.0",
    "knex": "^3.1.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
